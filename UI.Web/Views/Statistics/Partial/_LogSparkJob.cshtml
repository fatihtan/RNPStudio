@using Sky.CMS.Services
@using Sky.Log.DTO
@model UI.Web.Models.Statistics.LogListModel
@{
    Layout = null;
    ViewBag.Title = "Spark Job";
    List<LogSparkJob> LogList = ((List<BaseDTO>)Model.LogList).Cast<LogSparkJob>().ToList();
}

<table class="table table-striped table-bordered table-hover table-checkable order-column" id="item_list">
    <thead>
        <tr>
            <th class="hidden"></th>
            <th class="col-md-2">TKJob</th>
            <th class="col-md-1">ActorID</th>
            <th class="col-md-1">TKActor</th>
            <th class="col-md-1 text-center">Durum</th>
            <th class="col-md-1">JobID</th>
            <th>Açıklama</th>
            <th class="col-md-1 text-center">IP Adres</th>
            <th class="col-md-1 text-center">Tarih</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in LogList)
        {
            <tr class="odd gradeX object-item" data-id="" id="object_@item.ID">
                <td class="hidden">&nbsp;</td>
                <td>@item.TKJob.ToString()</td>
                <td>@item.ActorID</td>
                <td>@item.TKActor.ToString()</td>
                <td class="text-center">
                    <span class="label label-sm label-@(item.IsSuccess ? "info" : "danger")">
                        @(item.IsSuccess ? "Evet" : "Hayır")
                    </span>
                </td>
                <td>@item.JobID</td>
                <td>@item.Description</td>
                <td class="text-center">@item.IPAddress</td>
                <td class="text-center">@item.CreatedAt.ToString("yyyy.MM.dd HH:mm:ss")</td>
            </tr>
        }

    </tbody>
</table>

<input type="hidden" id="orderNum" value="8" />